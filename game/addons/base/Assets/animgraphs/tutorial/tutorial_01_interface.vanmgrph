					m_vecPosition = [ 1024.0, 480.0 ]
					m_nNodeID = 
					{
						m_id = 1498098042
					}
					m_networkMode = "ServerAuthoritative"
					m_sNote = ""
					m_commentText = "To assign a default animgraph to your model file (.vmdl), open it in ModelDoc, and look at the top-right of the window, in the \'Outliner\'; there\'s a field there for you to put the path to your graph.\n\nWe\'ve also added the ability for you to dynamically define or change the animgraph of a model in gameplay code.\n\nFor example:\n\nViewModelEntity.SetAnimGraph( \"models/first_person/first_person_arms_punching.vanmgrph\" );"
