				value = 
				{
					_class = "CCommentAnimNode"
					m_sName = "03. \'On Finished\' & \'On Almost Finished\'"
					m_vecPosition = [ -5440.0, 160.0 ]
					m_nNodeID = 
					{
						m_id = 176973187
					}
					m_networkMode = "ServerAuthoritative"
					m_sNote = ""
					m_commentText = "That\'s right! We\'re already going to talk about them again. This is why we just learned about them.\n\nSee, here\'s the problem. When is your state machine \'finishing\'\? \n\nFor it to \'finish\', it needs at least one end state.\n\nOnce it\'s on one of those end states, the timing info will get passed downstream to the next state machine (the one further down the proverbial \'tree\', so technically the \'lower level\' one), and the finished condition(s) will be able to make use of it.\n\nPeek into the \'Slide enter/exit\' SM to see how this is set up."
					m_size = [ 704.0, 448.0 ]
					m_color = [ 93, 99, 39, 255 ]
				}
			},
			{
				key = 
				{
					m_id = 362273380
				}
				value = 
				{
					_class = "CCommentAnimNode"
					m_sName = "06. \'Normal movement\' SM vs. \'Sliding\' SM"
					m_vecPosition = [ -4032.0, -736.0 ]
					m_nNodeID = 
					{
						m_id = 362273380
					}
					m_networkMode = "ServerAuthoritative"
					m_sNote = ""
					m_commentText = "You may notice these two state machines are nearly identical. And that\'s  because they manage jumping/airborne/landing relative to their current state. \n\nBut imagine if all eight of these states had to coexist in the same state machine.\n\nImagine the number of transitions you would need"
					m_size = [ 736.0, 384.0 ]
					m_color = [ 37, 66, 104, 255 ]
				}
